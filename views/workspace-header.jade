.masthead
	ul.nav.nav-pills.pull-right
		each page in pages
			if current == page
				li.active
					a('href'='/'+page.toLowerCase()) #{page}
			else
				li
					a('href'='/'+page.toLowerCase()) #{page}
		li
			a('href'='/home') Leave Room
	h3.muted
		a(href="/").home_link
			|Pair
		a(href="/").home_link_highlight
			|IDE
